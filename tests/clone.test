#!/bin/sh

source ./scripts/clone
source ./tests/test_utils

# It clones the branch in a folder named "<branch>-tmp>"
testClone() {
  local tmpDir=$(makeTempDir clone)
  local path=$(clone_and_build $tmpDir https://github.com/Smartesting/gravity-data-collector.git main)
  assertEquals $path "$tmpDir/main-tmp"

  assertFiles $tmpDir "main-tmp"
}
